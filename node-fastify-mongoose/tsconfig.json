{
    "compilerOptions": {
        "target": "ES2015",
        "sourceMap": true,
        "module": "commonjs",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended",
    "extends": "@tsconfig/node14/tsconfig.json"
}